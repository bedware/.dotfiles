{
    "Total Commander": {
        "selector": "ahk_exe TOTALCMD64.EXE",
        "commands": {
            "re": { "keys": "{F2}",  "desc": "Rename" },
            "q":  { "keys": "{F3}",  "desc": "Quick View" },
            "v":  { "keys": "{F3}",  "desc": "Quick View" },
            "e":  { "keys": "{F4}",  "desc": "Edit" },
            "cp": { "keys": "{F5}",  "desc": "Copy" },
            "mv": { "keys": "{F6}",  "desc": "Move" },
            "md": { "keys": "{F7}",  "desc": "Make dir" },
            "mf": { "keys": "+{F4}", "desc": "Make file" },
            "mk": { "keys": "{F7}",  "desc": "Make dir" },
            "rm": { "keys": "{F8}",  "desc": "Remove" },
            "1":  { "keys": "!{F1}", "desc": "Select left drive" },
            "2":  { "keys": "!{F2}", "desc": "Select right drive" }
        }
    },
    "Windows Terminal": {
        "selector": "ahk_exe WindowsTerminal.exe",
        "commands": {
            "d": { "keys": "^!1",  "desc": "Go to .dotfiles" },
            "f": { "keys": "^!1",  "desc": "Go to .dotfiles" },
            "a": { "keys": "^!2",  "desc": "Go to AHK" },
            "n": { "keys": "^!3",  "desc": "Go to Neovim" },
            "c": { "keys": "^!4",  "desc": "Go to Console" }
        }
    },
    "Intellij Idea": {
        "selector": "ahk_exe idea64.exe",
        "commands": {
            "sa": { "keys": "^+a",  "desc": "Show actions" },
            "re": { "keys": "+{F6}",  "desc": "Rename" },
            "fr": { "keys": "^+r",  "desc": "Replace in files" },
            "fi": { "keys": "^+f",  "desc": "Find in files" },
            "push": { "keys": "!p",  "desc": "Commit and Push" }
        }
    }
}
