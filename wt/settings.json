        {
            "command": "openNewTabDropdown",
            "keys": "menu"
        },
        {
            "command": "openTabRenamer",
            "keys": "alt+r"
        },
        {
            "command": "find",
            "keys": "ctrl+/"
        },
        {
            "command": 
            {
                "action": "splitPane",
                "split": "down"
            },
            "keys": "alt+minus"
        },
        {
            "command": "toggleFocusMode",
            "keys": "alt+f"
        },
        {
            "command": 
            {
                "action": "moveFocus",
                "direction": "left"
            },
            "keys": "alt+h"
        },
        {
            "command": 
            {
                "action": "moveFocus",
                "direction": "right"
            },
            "keys": "alt+l"
        },
        {
            "command": "unbound",
            "keys": "f11"
        },
        {
            "command": "unbound",
            "keys": "ctrl+shift+w"
        },
        {
            "command": "unbound",
            "keys": "ctrl+shift+f"
        }
