    "actions": 
    [
        {
            "command": 
            {
                "action": "sendInput",
                "input": "cd ..\r"
            },
            "keys": "alt+u"
        },
        {
            "command": 
            {
                "action": "splitPane",
                "split": "right"
            },
            "keys": "alt+\\"
